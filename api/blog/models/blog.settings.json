{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "prawo"
      ],
      "required": true,
      "default": "prawo"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date"
    }
  }
}
